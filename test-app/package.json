    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "typechain": "^5.1.2"
  },
  "pre-commit": [
    "prettier: check"
  ],
  "eslintIgnore": [
    "README.md",
    "yarn.lock",
    "/build"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*": [
      "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
      "prettier --write"
    ]
  }
